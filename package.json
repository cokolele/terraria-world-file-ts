{
  "name": "terraria-world-file",
  "version": "1.0.1",
  "main": "./dist/index.cjs",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^24.3.0",
    "prettier": "^3.6.2",
    "tsdown": "^0.16.0"
  },
  "scripts": {
    "build:prod": "tsdown src/index.ts src/platform/node.ts src/platform/browser.ts --format esm --format cjs --clean --exports",
    "build:watch": "tsdown src/index.ts src/platform/node.ts src/platform/browser.ts --format esm --format cjs --clean --watch"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "source": "./src/index.ts"
    },
    "./platform/browser": {
      "import": "./dist/platform/browser.mjs",
      "require": "./dist/platform/browser.cjs",
      "source": "./src/platform/browser.ts"
    },
    "./platform/node": {
      "import": "./dist/platform/node.mjs",
      "require": "./dist/platform/node.cjs",
      "source": "./src/platform/node.ts"
    }
  },
  "dependencies": {
    "typescript": "^5.9.3"
  },
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts"
}
